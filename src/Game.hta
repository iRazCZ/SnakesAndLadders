if (seed > 100)
 {
seed--;
cmd="scrollit("+seed+")";
timerTwo=window.setTimeout(cmd,100);
}

else 

if (seed <= 100 && seed > 0)
 {

for (c=0 ; c < seed ; c++)
{
out+=" ";
}

out+=msg;
seed--;
window.status=out;
cmd="scrollit("+seed+")";
timerTwo=window.setTimeout(cmd,100);

}

else 

if (seed <= 0)
 {

if (-seed < msg.length)
 {
